{
  "name": "mobx-select",
  "version": "2.2.1",
  "license": "WTFPL",
  "description": "MobX and MobX-State-Tree store selector for using with mobx-react-lite",
  "repository": "farwayer/mobx-select",
  "author": "farwayer <farwayer@gmail.com>",
  "scripts": {
    "prepack": "yarn cjs && yarn esm && size-limit",
    "cjs": "babel --out-file index.cjs.js index.js",
    "esm": "cross-env ESM=1 babel --out-file index.esm.js index.js"
  },
  "type": "module",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "react-native": "index.js",
  "sideEffects": false,
  "dependencies": {
    "istp": "^1.0.6"
  },
  "peerDependencies": {
    "mobx-react-lite": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@size-limit/preset-small-lib": "^4.10.2",
    "cross-env": "^7.0.3",
    "size-limit": "^4.10.2"
  },
  "files": [
    "index*.js"
  ],
  "keywords": [
    "mobx",
    "mobx-react",
    "mobx-react-lite",
    "inject",
    "store",
    "select",
    "selector",
    "observer",
    "mobx-state-tree"
  ],
  "size-limit": [
    {
      "path": "index.esm.js",
      "import": "{select, StoreProvider}",
      "ignore": [
        "react"
      ],
      "limit": "1.7 KB"
    }
  ]
}
