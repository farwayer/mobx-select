{
  "name": "mobx-select",
  "version": "2.1.3",
  "license": "WTFPL",
  "description": "MobX and MobX-State-Tree store selector for using with mobx-react-lite",
  "repository": "farwayer/mobx-select",
  "author": "farwayer <farwayer@gmail.com>",
  "scripts": {
    "prepack": "yarn cjs && yarn esm",
    "cjs": "babel --out-file index.cjs.js index.js",
    "esm": "cross-env ESM=1 babel --out-file index.esm.js index.js"
  },
  "type": "module",
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "react-native": "index.js",
  "sideEffects": false,
  "dependencies": {
    "istp": "^1.0.5",
    "react-is": "^17.0.1"
  },
  "peerDependencies": {
    "mobx-react-lite": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "cross-env": "^7.0.2"
  },
  "files": [
    "index*.js"
  ],
  "keywords": [
    "mobx",
    "mobx-react",
    "mobx-react-lite",
    "inject",
    "store",
    "select",
    "selector",
    "observer",
    "mobx-state-tree"
  ]
}
